{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "singularName": "product",
    "pluralName": "products",
    "displayName": "product",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "title": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "is_featured": {
      "type": "integer",
      "default": 0,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "is_hot": {
      "type": "integer",
      "default": 0,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "price": {
      "type": "decimal",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "sale_price": {
      "type": "decimal",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "vendor": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "review": {
      "type": "integer",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "is_out_of_stock": {
      "type": "boolean",
      "default": false,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "depot": {
      "type": "biginteger",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "inventory": {
      "type": "biginteger",
      "default": "0",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "is_active": {
      "type": "boolean",
      "default": true,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "is_sale": {
      "type": "boolean",
      "default": false,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "slug": {
      "type": "uid",
      "targetField": "bar_code"
    },
    "collection": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::collection.collection",
      "inversedBy": "products"
    },
    "product_category": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::product-category.product-category",
      "inversedBy": "products"
    },
    "brand": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::brand.brand",
      "inversedBy": "products"
    },
    "product_imgs": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ],
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "bar_code": {
      "type": "string",
      "required": true,
      "unique": true,
      "minLength": 6,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "descriptions": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "text"
    }
  }
}
